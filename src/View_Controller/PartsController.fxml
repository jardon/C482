<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.PartsController">
   <children>
      <VBox layoutX="-3.0" />
      <HBox layoutX="8.0" prefHeight="34.0" prefWidth="584.0">
         <children>
            <Label text="Add Part">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font></Label>
            <RadioButton fx:id="inHouse" mnemonicParsing="false" text="In-House">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </HBox.margin></RadioButton>
            <RadioButton fx:id="outsourced" mnemonicParsing="false" text="Outsourced">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </HBox.margin></RadioButton>
         </children>
      </HBox>
      <HBox layoutX="14.0" layoutY="55.0" prefHeight="305.0" prefWidth="573.0">
         <children>
            <VBox prefHeight="356.0" prefWidth="178.0">
               <children>
                  <Label text="ID">
                     <VBox.margin>
                        <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                     </VBox.margin></Label>
                  <Label text="Name">
                     <VBox.margin>
                        <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                     </VBox.margin></Label>
                  <Label text="Inventory">
                     <VBox.margin>
                        <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                     </VBox.margin></Label>
                  <Label text="Price/Cost">
                     <VBox.margin>
                        <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                     </VBox.margin></Label>
                  <Label text="Max">
                     <VBox.margin>
                        <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                     </VBox.margin></Label>
                  <Label text="Company Name">
                     <VBox.margin>
                        <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                     </VBox.margin></Label>
               </children>
            </VBox>
            <VBox prefHeight="305.0" prefWidth="395.0">
               <children>
                  <TextField fx:id="id">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <TextField fx:id="name">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <TextField fx:id="inventory">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <TextField fx:id="price">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <HBox prefHeight="33.0" prefWidth="395.0">
                     <children>
                        <TextField fx:id="max" />
                        <Label text="Min">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin></Label>
                        <TextField fx:id="min" />
                     </children>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </HBox>
                  <TextField fx:id="company">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin></TextField>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox layoutX="16.0" layoutY="342.0" prefHeight="55.0" prefWidth="573.0">
         <children>
            <Button fx:id="save" mnemonicParsing="false" onAction="#savePartAction" text="Save">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancelAction" text="Cancel">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
